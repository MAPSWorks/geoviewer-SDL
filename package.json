	"preBuildCommands": [
		"echo 'module gitinfo;' > src//gitinfo.d",
		"echo '' >> src//gitinfo.d",
		"echo '// DO NOT EDIT!' >> src//gitinfo.d",
		"echo '// File generated by preBuildCommands section of dub package file.' >> src//gitinfo.d",
		"echo '' >> src//gitinfo.d",
		"echo 'immutable gitCommit = \"' \"`git rev-parse HEAD`\" '\";' >> src//gitinfo.d",
		"echo 'immutable gitDatetime = \"' \"`git log -1 --format=\"%ci \"`\" '\";' >> src//gitinfo.d"
	]
}
